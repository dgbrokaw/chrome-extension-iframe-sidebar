{
	"manifest_version": 2,
	"name": "Test Extension",
	"version": "0.4",

	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "Test Extension"
	},

	"content_scripts": [
		{
			"matches": ["http://*/*", "https://*/*", "file://*/*"],
			"js": ["main.js"]
		}
	],

	"permissions": [
		"tabs",
		"http://*/*",
		"https://*/*"
	]
}
