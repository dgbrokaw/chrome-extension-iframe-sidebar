{
	"manifest_version": 2,
	"name": "Test Extension",
	"version": "0.8",

	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "Test Extension"
	},

	"background": {
		"scripts": ["background.js"],
		"persistent": true
	},

	"web_accessible_resources": [
		"libs/jquery/jquery-3.1.0.min.js"
	, "libs/bootstrap/js/bootstrap.min.js"
	,	"sidebar.html"
	, "iframe.js"
	],

	"permissions": [
		"tabs",
		"webNavigation",
		"http://*/*",
		"https://*/*"
	]
}
